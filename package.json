    "express-graphql": "^0.8.0",
    "graphql": "^14.2.1",
    "hapi": "^18.1.0",
    "inert": "^5.1.3",
    "joi": "^14.3.1",
    "lab": "^18.0.2",
    "lodash": "^4.17.11",
    "mongoose": "^5.5.2",
    "nodemon": "^1.18.11",
    "relish": "^3.0.0",
    "vision": "^5.4.4"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
