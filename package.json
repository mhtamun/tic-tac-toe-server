    "type": "git",
    "url": "git+https://github.com/mhtamun/tic-tac-toe-server.git"
  },
  "bugs": {
    "url": "https://github.com/mhtamun/tic-tac-toe-server/issues"
  },
  "scripts": {
    "start-hapi": "nodemon src/hapi-server/index.js",
    "start-express": "nodemon src/express-server/app.js",
    "start": "npm run start-express",
    "test": "lab src/hapi-server/tests/ --timeout 3000 --assert code --leaks"
  },
  "dependencies": {
    "apollo-server-hapi": "^2.4.8",
    "boom": "^7.3.0",
    "code": "^5.2.4",
